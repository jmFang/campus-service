/**app.wxss**/
/*作用于所有page*/
@import 'weui.wxss';
page{
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  font-size: 10pt;
  line-height: 150%;
  min-height: 100%;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: stretch;
}

.container {
  position: relative;
  flex:1;
  min-height: 100%;
  display: flex;
  flex-direction: column;
  padding-bottom: 100rpx;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
